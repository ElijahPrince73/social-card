{
	"parser": "babel-eslint",
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": ["error", {
			"trailingComma": "es5",
			"semi": false,
			"singleQuote": true
		}]
	},
	"env": {
		"node": true,
		"browser": true
	}
}